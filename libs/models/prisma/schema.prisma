generator client {
  provider = "prisma-client-js"
}

datasource db {
  url      = env("DATABASE_URL")
  provider = "postgres"
}

model User {
  id           String @id @default(uuid())
  username     String @unique @db.VarChar(255)
  emailAddress String @unique @db.VarChar(320)
  password     String @db.VarChar(255)
}
